    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.39.8",
    "expo": "53.0.11",
    "expo-av": "~15.1.6",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.8",
